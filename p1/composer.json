{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Cottontail Laravel Framework Template",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "barryvdh/laravel-async-queue": "^0.7.5",
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.10",
        "barryvdh/laravel-snappy": "^0.4.8",
        "barryvdh/laravel-translation-manager": "^0.5.10",
        "diglactic/laravel-breadcrumbs": "^6.1",
        "fideloper/proxy": "^4.4",
        "filp/whoops": "^2.12",
        "fruitcake/laravel-cors": "^2.0",
        "fruitcake/laravel-telescope-toolbar": "^1.3",
        "guzzlehttp/guzzle": "^7.3",
        "infureal/artisan-gui": "^2.0",
        "laravel/framework": "^8.40",
        "laravel/telescope": "^4.4",
        "laravel/tinker": "^2.6",
        "laravelista/picasso": "^0.8.1",
        "nelson6e65/phpdoc-vuepress": "^1.0",
        "nunomaduro/laravel-console-task": "^1.6",
        "phpdocumentor/phpdocumentor": "^3.0",
        "symfony/flex": "^1.13"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.4",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "clean-with-update": [
            "composer status -v",
            "composer update",
            "php artisan cache:clear",
            "php artisan routes:clear"
        ],
        "local-dev": [
            "vuepress dev --host localhost",
            "php artisan serve"
        ],
        "build docs": [
            "php artisan route:cache",
            "vuepress build docs"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
